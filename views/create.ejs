                                <option value="9 - Bioremediation">Bioremediation</option>
                                <option value="9 - Ecosystems Management, Forestry">Ecosystems Management, Forestry</option>
                                <option value="9 - Land Reclamation">Land Reclamation</option>
                                <option value="9 - Pollution Control: Air, Water, Soil">Pollution Control: Air, Water, Soil</option>
                                <option value="9 - Recycling and Waste Management">Recycling and Waste Management</option>
                                <option value="9 - Water Resources Management">Water Resources Management</option>
                              </optgroup>
                              <optgroup label="Earth and Environmental Sciences">
                                <option value="10 - Atmospheric Science, Weather">Atmospheric Science, Weather</option>
