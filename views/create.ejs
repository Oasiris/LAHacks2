                                <option value="1 - Clinical and Cognitive Psychology">Clinical &amp; Cognitive Psychology</option>
                                <option value="1 - Clinical and Physiological Psychology">Clinical &amp; Physiological Psychology</option>
                                <option value="1 - Clinical and Developmental Psychology">Clinical &amp; Developmental Psychology</option>
                                <option value="1 - Clinical and Social Psychology">Clinical &amp; Social Psychology</option>
                              </optgroup>
